{
  "name": "petmeds-backend",
  "version": "1.0.0",
  "description": "Backend API for PetMeds application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "db:setup": "ts-node src/scripts/setup-db.ts",
    "db:migrate": "ts-node src/scripts/setup-db.ts",
    "db:seed": "ts-node src/scripts/seed-db.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "google-auth-library": "^9.0.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "express-rate-limit": "^7.1.5",
    "web-push": "^3.6.6",
    "node-cron": "^3.0.3",
    "moment-timezone": "^0.5.43",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/pg": "^8.10.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@types/web-push": "^3.6.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "keywords": ["pet", "medication", "api", "express", "typescript"],
